<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:controls="clr-namespace:Magitek.Controls">

    <Style x:Key="JobOptionsTabControl" TargetType="{x:Type TabControl}">
        <Setter Property="TabStripPlacement" Value="Left" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type TabControl}">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>

                        <Rectangle Grid.Column="0" Width="1.5" HorizontalAlignment="Right" VerticalAlignment="Stretch" Fill="{DynamicResource TabDividerVerticalRight}" />

                        <TabPanel Grid.Column="0" IsItemsHost="true" />

                        <Border Grid.Column="1" Background="{DynamicResource BackgroundContent}">
                            <ContentPresenter ContentSource="SelectedContent" />
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="CardRulesTabControl" TargetType="{x:Type TabControl}">
        <Setter Property="TabStripPlacement" Value="Top" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type TabControl}">
                    <Grid HorizontalAlignment="Stretch">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition />
                        </Grid.RowDefinitions>

                        <controls:SettingsBlock Grid.Row="0" HorizontalAlignment="Center" Background="{DynamicResource ClassSelectorBackground}">
                            <TabPanel IsItemsHost="true" />
                        </controls:SettingsBlock>

                        <controls:SettingsBlock Grid.Row="1" Margin="0,5" Background="{DynamicResource ClassSelectorBackground}">
                            <ContentPresenter ContentSource="SelectedContent" />
                        </controls:SettingsBlock>

                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

