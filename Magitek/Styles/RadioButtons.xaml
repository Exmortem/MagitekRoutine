                                <Path Name="Checked"
                                      Height="8"
                                      Data="F1 M 0,110L 47.6667,62L 102,117.333L 218.667,0L 266,48L 102,212.333L 0,110 Z"
                                      Fill="{DynamicResource Background}"
                                      Opacity="0.90"
                                      Stretch="Uniform"
                                      UseLayoutRounding="False"
                                      Visibility="Hidden">
                                </Path>

                                <Path Name="Unchecked"
                                      Height="8"
